<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.metoo.nspm.core.mapper.nspm.MacVendorMapper">

    <select id="selectObjByMap" parameterType="java.util.Map" resultType="com.metoo.nspm.entity.nspm.MacVendor">
        SELECT *
        FROM metoo_mac_vendor
        <where>
            <if test="mac != null and mac != ''">
                AND mac = #{mac}
            </if>
        </where>

    </select>
</mapper>